{
  "name": "mr",
  "version": "1.0.0",
  "description": "Server rendering VueJs project",
  "author": "Mykhailov Pavel <mykhailovpm@gmail.com>",
  "private": true,
  "engines": {
    "node": "10.8.0",
    "npm": "6.2.0"
  },
  "scripts": {
    "dev": "export NODE_ENV=development && backpack dev",
    "test": "export NODE_ENV=test && mocha --require babel-core/rwegister test/server.test.js --exit",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production && node build/main.js && npm run update-categories",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "heroku-postbuild": "npm run build",
    "update-categories": "node scripts/update-categories.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.11.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "formidable": "^1.2.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mailgun-js": "^0.17.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.2.7",
    "nuxt": "^1.4.2",
    "regenerator-runtime": "^0.11.1",
    "source-map-support": "^0.4.15",
    "validator": "^10.5.0",
    "vee-validate": "^2.0.9",
    "vue-style-loader": "^4.1.1",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "backpack-core": "^0.4.1",
    "bootstrap": "^4.1.3",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^6.0.7",
    "supertest": "^3.1.0"
  }
}
