{
  "name": "mr",
  "version": "1.0.0",
  "description": "Server rendering VueJs project",
  "author": "Mykhailov Pavel <mykhailovpm@gmail.com>",
  "private": true,
  "engines": {
    "node": "9.8.0",
    "npm": "5.8.0"
  },
  "scripts": {
    "dev": "export NODE_ENV=development && backpack dev",
    "test": "export NODE_ENV=test && mocha --require babel-core/register test/server.test.js --exit",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production && node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "body-parser": "^1.18.2",
    "cloudinary": "^1.11.0",
    "cross-env": "^5.0.1",
    "express": "^4.15.3",
    "formidable": "^1.2.1",
    "mailgun-js": "^0.17.0",
    "mocha": "^5.0.4",
    "mongoose": "^5.0.10",
    "nuxt": "^1.0.0-rc3",
    "regenerator-runtime": "^0.11.1",
    "source-map-support": "^0.4.15",
    "vee-validate": "^2.0.6",
    "vue-style-loader": "^4.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "backpack-core": "^0.4.1",
    "chai": "^4.1.2",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "supertest": "^3.0.0"
  }
}
